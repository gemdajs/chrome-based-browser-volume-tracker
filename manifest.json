{
    "manifest_version": 3,
    "name": "Video Volume + Inactivity Tracker",
    "version": "1.2",
    "permissions": ["storage", "tabs", "windows"],
    "action": {"default_popup": "popup.html"},
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ]
}